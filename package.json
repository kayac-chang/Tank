    "@babel/preset-flow": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "copy-webpack-plugin": "^5.0.2",
    "css-loader": "^2.1.1",
