    "eslint-config-google": "^0.12.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-flowtype": "^3.2.1",
    "express": "^4.16.4",
    "file-loader": "^3.0.1",
    "flow-bin": "^0.92.1",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "mini-css-extract-plugin": "^0.5.0",
    "minimist": "^1.2.0",
    "sass": "^1.22.8",
    "sass-loader": "^7.1.0",
    "url-loader": "^1.1.2",
